Template
name 	DocType
debugOn	false
format	<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">

Template
name 	HtmlContent
format	<html xmlns="http://www.w3.org/1999/xhtml">

Template
name 		HtmlHead
format	<head>
 <meta http-equiv="Content-type" content="text/html;charset=UTF-8"/>
 <link rel="stylesheet" type="text/css" href="dmsstyle.css" />
 <title>The ::schemaName:: Schema</title>
 </head>

Template
name		DivHeader
format		<div id="header"> <div class="headerBar"> </div> </div>

Template
name		SchemaTitle
format		<div class="schemaName"> ::schemaName:: </div>

Template
name		ClassSummary
format		<div class="classList">
 <h2> Classes (::classCount::)</h2>
 <table>

Template
name		ClassListEnd
format		</table>
 </div> <!-- classList -->


